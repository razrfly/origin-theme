{
  "routes": [
    {
      "src": "/blog",
      "dest": "https://blog.teamups.co/"
    },

    {
      "src": "/blog/(.*?)([.]\\w{2,5})(\\?[^/]+)?",
      "dest": "https://blog.teamups.co/$1$2$3"
    },

    {
      "src": "/blog/([\\S\\s]+)",
      "dest": "https://blog.teamups.co/$1/"
    }
  ]
}
